---
log_file: /var/log/clickhouse-proxy/general.log
log_level: INFO
log_dir_debug: /var/log/clickhouse-proxy/detailed
listen_host: 0.0.0.0
listen_port: 8000
clickhouse_scheme: http
clickhouse_host: localhost
clickhouse_port: 8123
encoding: utf-8
users:
  - name: default
    readonly: False
    ip:
      # Allow connections only from these IPs
      - 127.0.0.1
